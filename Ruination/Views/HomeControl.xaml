<UserControl x:Class="Ruination_v2.Views.HomeControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Ruination_v2.Views"
             mc:Ignorable="d" 
             d:DesignHeight="650" d:DesignWidth="800" Background="Transparent">
    <Grid>
        <Label Content="Welcome to Ruination" Foreground="White" FontWeight="Bold"></Label>
        <MediaElement Margin="0,31,0,0" SpeedRatio="5" IsMuted="False" x:Name="newsImage" LoadedBehavior="Manual"></MediaElement>
    </Grid>
</UserControl>
